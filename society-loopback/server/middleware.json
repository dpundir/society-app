{
    "initial:before": {
        "loopback#favicon": {}
    },
    "initial": {
        "compression": {},
        "cors": {
            "params": {
                "origin": true,
                "credentials": true,
                "maxAge": 86400
            }
        }
    },
    "session": {},
    "auth": {},
    "parse": {
        "body-parser#json": {
            "params": {
                "extended": true
            }
        },
        "body-parser#urlencoded": {
            "params": {
                "extended": true
            }
        },
        "body-parser#raw": {
            "params": {
                "extended": true
            }
        }
    },
    "routes": {
        "loopback#rest": {
            "paths": [
                "${restApiRoot}"
            ]
        }
    },
    "files": {
        "loopback#static": {
            "params": "$!../client"
        }
    },
    "final": {
        "loopback#urlNotFound": {}
    },
    "final:after": {
        "loopback#errorHandler": {}
    }
}
